{
    "name": "chronomail/redundant_attachments",
    "type": "chronomail-plugin",
    "description": "This plugin provides a redundant storage for temporary uploaded attachment files. They are stored in both the database backend as well as on the local file system. It provides also memcache store as a fallback.",
    "license": "GPLv2",
    "version": "1.2",
    "authors": [
        {
            "name": "Aleksander Machniak",
            "email": "alec@alec.pl",
            "role": "Lead"
        },
        {
            "name": "Thomas Bruederli",
            "email": "chronomail@gmail.com",
            "role": "Lead"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.chronomail.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "chronomail/plugin-installer": ">=0.1.3",
        "chronomail/filesystem_attachments": ">=1.0.0"
    }
}
